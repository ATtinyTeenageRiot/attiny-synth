{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Encode Sample",
			"type": "python",
			"request": "launch",
			"stopOnEntry": false,
			"pythonPath": "${config.python.pythonPath}",
			"program": "${file}",
			"cwd": "${workspaceRoot}",
			"debugOptions": [
				"WaitOnAbnormalExit",
				"WaitOnNormalExit",
				"RedirectOutput"
			]
		}
	]
}